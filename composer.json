{
    "name": "laraket/me",
    "authors": [
        {
            "name": "neychang",
            "email": "zoobile@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/contracts": "~5.7.0|~5.8.0|~5.9.0",
        "illuminate/database": "~5.7.0|~5.8.0|~5.9.0",
        "illuminate/support": "~5.7.0|~5.8.0|~5.9.0",
        "ramsey/uuid": "^3.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.0"
    },
    "autoload":{
        "psr-4": {
            "Laraket\\Me\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laraket\\Me\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Laraket\\Me\\MeServiceProvider"
            ],
            "aliases": {
                
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
